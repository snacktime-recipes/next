{
  "name": "backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend/src",
  "projectType": "app",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ace serve --watch",
        "cwd": "apps/backend"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ace build --production",
        "cwd": "apps/backend"
      }
    }
  },
  "tags": []
}
  