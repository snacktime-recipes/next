{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web/src",
  "projectType": "app",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx concurrently \"npx vite dev\" \"npx typesafe-i18n\"",
        "cwd": "apps/web"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx vite build",
        "cwd": "apps/web"
      }
    }
  },
  "tags": []
}
    